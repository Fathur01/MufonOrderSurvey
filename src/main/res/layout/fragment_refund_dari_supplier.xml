<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.muf.mymuf.mobilesurvey.fragment.aplikasi.RefundDariSupplierFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin">

        <LinearLayout
            android:id="@+id/refund_dari_supplier_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="4dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/refund_dari_supplier_subsidi_dp"
                    android:textColor="@color/gray"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16dp"
                    android:layout_weight="1"
                    android:text="Subsidi DP From Supplier" />


                <TextView
                    android:id="@+id/txt_refund_dari_supplier_subsidi_dp"
                    android:textColor="@color/gray"
                    android:layout_width="150dp"
                    android:layout_marginRight="12dp"
                    android:text="0.00"
                    android:textSize="16dp"
                    android:inputType="numberDecimal"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:gravity="end"/>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="4dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/refund_dari_supplier_balik_nama"
                    android:textColor="@color/gray"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="16dp"
                    android:text="Biaya Balik Nama From Supplier" />


                <TextView
                    android:id="@+id/txt_refund_dari_supplier_balik_nama"
                    android:textColor="@color/gray"
                    android:layout_width="150dp"
                    android:layout_marginRight="12dp"
                    android:text="0.00"
                    android:textSize="16dp"
                    android:inputType="numberDecimal"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:gravity="end"/>

            </LinearLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_join_promo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_join_promo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="numberDecimal"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Join Promo From Supplier" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_admin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_admin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="numberDecimal"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Adm From Supplier" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_subsidi_rate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_subsidi_rate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:focusable="false"
                    android:drawableRight="@drawable/ic_arrow"
                    android:drawableEnd="@drawable/ic_arrow"

                    android:hint="Subsidi Rate From Supplier" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_subsidi_potong_pelunasan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_subsidi_potong_pelunasan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="numberDecimal"
                    android:focusable="false"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Subsidi Potong Pelunasan" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_subsidi_claim_aptm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_subsidi_claim_aptm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:inputType="numberDecimal"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Subsidi Claim Dana APTM" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_eff_sebelum_subsidi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_eff_sebelum_subsidi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:inputType="numberDecimal"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Eff. Rate Sebelum Subsidi" />

            </android.support.design.widget.TextInputLayout>
            <android.support.design.widget.TextInputLayout
                android:id="@+id/refund_dari_supplier_flat_sebelum_subsidi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp">

                <EditText
                    android:id="@+id/input_refund_dari_supplier_flat_sebelum_subsidi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:inputType="numberDecimal"
                    android:textColor="@color/colorPrimaryDark"
                    android:hint="Flat Rate Sebelum Subsidi" />

            </android.support.design.widget.TextInputLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>
