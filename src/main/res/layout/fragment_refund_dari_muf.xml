<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.muf.mymuf.mobilesurvey.fragment.aplikasi.InformasiBiayaFragment">

    <LinearLayout
        android:id="@+id/refund_dari_muf_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="4dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/refund_dari_muf_administrasi"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Administrasi" />


            <TextView
                android:id="@+id/txt_refund_dari_muf_administrasi"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="150dp"
                android:layout_marginRight="12dp"
                android:text="-"
                android:inputType="numberDecimal"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:gravity="end"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="4dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/refund_dari_muf_balik_nama"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Biaya Balik Nama" />


            <TextView
                android:id="@+id/txt_refund_dari_muf_balik_nama"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="150dp"
                android:layout_marginRight="12dp"
                android:text="-"
                android:inputType="numberDecimal"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:gravity="end"/>

        </LinearLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/refund_dari_muf_komisi_langsung_supplier"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/input_refund_dari_muf_komisi_langsung_supplier"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:textColor="@color/colorPrimaryDark"
                android:hint="Komisi Langsung Untuk Supplier"/>

        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/refund_dari_muf_discount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/input_refund_dari_muf_discount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:textColor="@color/colorPrimaryDark"
                android:hint="Discount"/>

        </android.support.design.widget.TextInputLayout>
        <LinearLayout
            android:id="@+id/layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="4dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/refund_dari_muf_komisi_langsung_channel"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Komisi Langsung Untuk Channel" />


            <TextView
                android:id="@+id/txt_refund_dari_muf_komisi_langsung_channel"
                android:textColor="@color/gray"
                android:textSize="12dp"
                android:layout_width="150dp"
                android:layout_marginRight="12dp"
                android:text="-"
                android:inputType="numberDecimal"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:gravity="end"/>

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
